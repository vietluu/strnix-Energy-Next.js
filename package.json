{
  "name": "strnix",
  "license": "MIT",
  "version": "1.1.0",
  "private": true,
  "author": "vietluu",
  "engines": {
    "node": ">=16.8.0",
    "yarn": ">=1.22.0",
    "npm": "please-use-yarn"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "main": "server.js",
  "lint-staged": {
    "*.tsx": [
      "eslint --no-ignore --fix",
      "git add --force"
    ],
    "*.{json,md}": [
      "prettier --write",
      "git add --force"
    ]
  },
  "browserslist": [
    "chrome 64",
    "edge 79",
    "firefox 67",
    "opera 51",
    "safari 12"
  ],
  "scripts": {
    "dev": "next dev --turbo",
    "build": "next build",
    "start": "SET ENVIRONMENT=./.env &&  node server.js",
    "lint": "next lint",
    "prettier": "prettier --write .",
    "prepare": "husky install",
    "analyze": "ANALYZE=true next build",
    "prod": "NODE_ENV=production  node server.js"
  },
  "dependencies": {
    "@babel/polyfill": "^7.12.1",
    "@fancyapps/ui": "^5.0.19",
    "@next/bundle-analyzer": "^13.4.2",
    "@types/aos": "^3.0.4",
    "@types/node": "20.1.2",
    "@types/react": "18.2.6",
    "@types/react-countup": "^4.3.1",
    "@types/react-dom": "18.2.4",
    "@types/react-slick": "^0.23.10",
    "@types/slick-carousel": "^1.6.37",
    "antd": "^5.5.0",
    "aos": "^2.3.4",
    "autoprefixer": "10.4.14",
    "bootstrap": "^5.2.3",
    "eslint-config-next": "^13.4.2",
    "express": "^4.18.2",
    "jsdom": "^22.0.0",
    "next": "13.4.1",
    "next-compose-plugins": "^2.2.1",
    "nextjs-toploader": "^1.4.2",
    "postcss": "8.4.23",
    "react": "18.2.0",
    "react-countup": "^6.4.2",
    "react-device-detect": "^2.2.3",
    "react-dom": "18.2.0",
    "react-slick": "^0.29.0",
    "sass": "^1.62.1",
    "slick-carousel": "^1.8.1",
    "tailwindcss": "3.3.2",
    "typescript": "5.0.4"
  },
  "devDependencies": {
    "clean-css": "^5.3.2",
    "css-minimizer-webpack-plugin": "^5.0.0",
    "eslint": "^8.40.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-webpack-plugin": "^4.0.1",
    "file-loader": "^6.2.0",
    "husky": "^8.0.3",
    "img-optimize-loader": "^1.0.7",
    "lint-staged": "^13.2.2",
    "prettier": "^2.8.8",
    "terser-webpack-plugin": "^5.3.8",
    "thread-loader": "^4.0.2"
  }
}
